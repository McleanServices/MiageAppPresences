{
  "expo": {
    "name": "presences",
    "slug": "presences",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "presences",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "appleTeamId": "S288VRTCF4",
      "supportsTablet": true,
      "bundleIdentifier": "com.mcleantyrece.presences",
      "entitlements": {
        "com.apple.security.application-groups": [
          "group.com.presencestyrece.mywidget"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mcleantyrece.presences"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite",
      "@bacons/apple-targets",
      [
        "expo-notifications",
        {
          "sounds": [
            "./assets/notification_sound.wav"
          ]
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "bundleIdentifier": "com.mcleantyrece.presences.widget",
                  "targetName": "widget",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.com.presencestyrece.mywidget"
                    ]
                  }
                }
              ]
            }
          }
        },
        "projectId": "c530216b-092a-430c-959d-0f2c5d8b882b"
      }
    },
    "owner": "mcleantyrece"
  }
}
